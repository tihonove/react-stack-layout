{
  "name": "@skbkontur/react-stack-layout",
  "version": "2.0.0",
  "description": "Flexbox based stack layouts for react",
  "license": "MIT",
  "author": "Eugene Tihonov <tihonov.ea@gmail.com>",
  "private": false,
  "scripts": {
    "build": "webpack --mode=production",
    "ladle-serve": "ladle serve",
    "prepublishOnly": "npm run build"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tihonove/react-stack-layout.git"
  },
  "bugs": {
    "url": "https://github.com/tihonove/react-stack-layout/issues"
  },
  "keywords": [
    "react",
    "layout management",
    "layouting"
  ],
  "files": [
    "README.md",
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.ts",
    "src/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@ladle/react": "^5.0.1",
    "@storybook/addon-actions": "^8.6.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.15",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
